{
  "name": "react-ace",
  "version": "6.3.2",
  "description": "A react component for Ace Editor",
  "main": "lib/index.js",
  "types": "types.d.ts",
  "scripts": {
    "prettier": "prettier --print-width 120 --single-quote --trailing-comma all   --write \"src/**\" \"example/*.js\"",
    "clean": "rimraf lib dist",
    "lint": "node_modules/.bin/eslint src/*",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js -o dist/react-ace.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js -o dist/react-ace.min.js --config webpack.config.production.js",
    "example": "webpack-dev-server --config webpack.config.example.js",
    "build:example": "webpack --config webpack.config.example.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "check": "npm run lint",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "mocha --require babel-register --require tests/setup.js tests/**/*.spec.js --exit",
    "coverage": "nyc npm run test"
  },
  "author": "James Hrisho",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "5.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "7.11.1",
    "husky": "^1.1.1",
    "jsdom": "^13.0.0",
    "mocha": "5.2.0",
    "nyc": "13.2.0",
    "prettier": "^1.14.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "pretty-quick": "^1.7.0",
    "rimraf": "2.6.3",
    "sinon": "6.0.1",
    "webpack": "4.29.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": ">=3.1.11"
  },
  "keywords": [
    "ace",
    "ace editor",
    "react-component",
    "react",
    "brace"
  ],
  "dependencies": {
    "brace": "^0.11.1",
    "diff-match-patch": "^1.0.4",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "peerDependencies": {
    "react": "^0.13.0 || ^0.14.0 || ^15.0.1 || ^16.0.0",
    "react-dom": "^0.13.0 || ^0.14.0 || ^15.0.1 || ^16.0.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/setup.js",
      "node_modules"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "reporter": [
      "lcov",
      "text-lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/securingsincity/react-ace.git"
  }
}
