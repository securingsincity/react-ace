{
  "name": "react-ace",
  "version": "2.1.1",
  "description": "A react component for Ace Editor",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "gulp build:umd",
    "build:umd:min": "gulp build:umd:min",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "preversion": "npm run clean",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "build_example": "gulp example",
    "build_watch": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "James Hrisho",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.21",
    "browserify": "^9.0.8",
    "gulp": "^3.8.10",
    "gulp-jshint": "^1.9.0",
    "gulp-react": "^2.0.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.2.0",
    "jest": "^0.1.37",
    "rimraf": "^2.4.2",
    "jsx-loader": "^0.12.2",
    "reactify": "^1.1.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "keywords": [
    "ace",
    "ace editor",
    "react-component",
    "react",
    "brace"
  ],
  "browserify": {
    "transform": [
      [
        "reactify"
      ]
    ]
  },
  "dependencies": {
    "brace": "^0.5.1"
  },
  "peerDependencies": {
    "react": ">=0.11.2 <1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/securingsincity/react-ace.git"
  }
}
